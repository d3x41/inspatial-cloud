{
  "name": "@inspatial/cloud",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "exports": {
    ".": "./mod.ts",
    "./extensions": "./extensions/mod.ts"
  },

  "imports": {
    "#/": "./src/",
    "#extensions/": "./extensions/",
    "#types/": "./src/types/",
    "#client/": "./client/"
  },
  "tasks": {
    "check": "deno publish --dry-run"
  },
  "lint": {
    "rules": {
      "tags": ["jsr", "recommended"],
      "include": ["explicit-module-boundary-types", "verbatim-module-syntax"],
      "exclude": ["no-explicit-any"]
    }
  }
}
